<Window x:Class="ComposerTools.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComposerTools.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Style="{DynamicResource WindowStyle_Dark}">
    <Grid HorizontalAlignment="Left" Height="423" Margin="-4,0,-4,-4" VerticalAlignment="Top" Width="800">
        <Button Content="Open FL Studio 20" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="121" Height="35" Style="{DynamicResource ButtonStyle_Dark}" Click="Button_Click"/>
    </Grid>
</Window>
